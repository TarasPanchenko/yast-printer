/**
 * File:        etc_cups_client.scr
 * Summary:     Agent for reading /etc/cups/client.conf file
 * Author:      Jiri Srain <jsrain@suse.cz>
 * Access:      read
 *
 * $Id$
 *
 */
.etc.cups.client_conf

`ag_ini(
    `IniAgent( "/etc/cups/client.conf",
	$[
	    "options"	: [ "global_values" ],
	    "comments"	: [ "^[ \t]*#.*$", "^[ \t]*$" ],
	    "params"    : [
		$[ "match" : [ "^[ \t]*([^ \t]+)[ \t]+(.*)$", "%s %s" ]],
		$[ "match" : [ "^[ \t]*([^ \t]+)[ \t]*(.*)$", "%s %s" ]],
	    ],
	]
))


