Config c1
    UseOption o1 Text c_t1
EndConfig

Option o1
    Pattern Type {file:upp} { %uo2%%o4% } Text t1
	UseOption o2 As uo2 Text subo2
	UseOption o4 Text subo4
EndOption

Option o2
    Pattern Type {file:upp} { >%o3%< } Text t2
	UseOption o3 Text o3
EndOption

Option o3
    Value Type {file:upp} { X } Text o3
EndOption

Option o4
    Value Type {file:upp} { Y } Text o4
EndOption
