Config c1
    UseOption o1 Text c_t1
    UseOption o2 Text c_t2
EndConfig

Option o1
    Pattern Type {file:upp} { DEVICE:%dev% } Text t1
	UseOption device As { dev } Text subo2
EndOption

Option o2
    Pattern Type {file:upp} { PAPERSIZE:%papersize% } Text t1
	UseOption paper As { papersize } Text subo3
EndOption

Option device
    ValueText { [0-9][a-z]* } Pattern { % }
EndOption

Option paper
    ValueText { [0-9][a-z]* } Pattern { % }
EndOption
