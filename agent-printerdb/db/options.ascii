



# begin options ####################################################################################################


# begin generic options #############################################################################################

Option { text-number }
        ValueText { [0-9]* } Pattern { % }
EndOption { text-number }

Option { text-uppercase }
        ValueText { [A-Z]* } Pattern { % }
EndOption { text-uppercase }

Option { text-lowercase }
        ValueText { [a-z]* } Pattern { % }
EndOption { text-lowercase }

Option { text-lowercase-number }
        ValueText { [0-9a-z]* } Pattern { % }
EndOption { text-lowercase-number }

Option { text-uppercase-number }
        ValueText { [0-9A-Z]* } Pattern { % }
EndOption { text-uppercase-number }

Option { text-letter } 
        ValueText { [A-Za-z]* } Pattern { % }
EndOption { text-letter } 

Option { text-alphanumeric }
        ValueText { [0-9A-Za-z]* } Pattern { % }
EndOption { text-alphanumeric }

Option { text-path }
        ValueText { [-_~!%+,./:;=0-9A-Za-z]* } Pattern { % }
EndOption { text-path }

# to be safe we use a simplified version for "Option { text-arbitrary }"
# which has none of the problematic characters which may cause interpretation by the bash
# i.e. "Option { text-arbitrary }" is in fact the same as "Option { text-path }"
#
# Option { text-arbitrary }
#         ValueText { [.]* } Pattern { % }
# EndOption { text-arbitrary }

Option { text-arbitrary }
        ValueText { [-_~!%+,./:;=0-9A-Za-z]* } Pattern { % }
EndOption { text-arbitrary }

# end generic options ###############################################################################################

