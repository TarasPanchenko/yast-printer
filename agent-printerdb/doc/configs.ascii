#
# configurations
#

Config { canon-bjc-6200-color }
    PasteConfig { canon-bjc-6100-color }
    UseConfig { canon-bjc-6200-color-special } Text { Color driver 6200 } Queue { color }
EndConfig { canon-bjc-6200-color }

Config { canon-bjc-6200-color-special }
    Param { -sDEVICE=stp }
    UseOption { gs-stcolor } Text { st-color }
EndConfig { canon-bjc-6200-color-special }

Config { canon-bjc-6100-mono }
	Param {
		-sDEVICE=bjc600
		-sBPP=1
		-sCOLOR=mono
	}
	UseOption { papersize } 		Text { Paper size (1) }
EndConfig { canon-bjc-6100-mono }

Config { canon-bjc-6100-photo }
	Param {
		-sDEVICE=stp
		-sCOLOR=color
	}
	UseOption { papersizemaxa4 } 		Text { Paper size (1) }
	UseOption { stp-photo-media } 		Text { Media type (2) }
EndConfig { canon-bjc-6100-mono }

Config { canon-bjc-6100-color }
	UseConfig { canon-bjc-600-color } 	Text { Lowest quality color (1) } Queue { color }
	UseConfig { canon-bjc-6100-stp } 	Text { Color driver stp (2) } Queue { color }
	UseConfig { canon-bjc-6100-uniprint } 	Text { Older color driver uniprint (3) } Queue { color }
	PasteConfig { non-existing }
EndConfig { canon-bjc-6100-color }

Config { canon-bjc-600-color }
	Param {
		-sDEVICE=bjc600
		-sBPP=1
		-sCOLOR=mono
	}
	UseOption { canon-bjc-600-color }	 Text { Bits per pixel (1) }
EndConfig { canon-bjc-600-color }

Config { canon-bjc-6100-stp }
	Param {
		-sDEVICE=stp
	}
	UseOption { stp-hue } 			Text { Hue (1) }
	UseOption { stp-int } 			Text { Int (1) -dPercentage="{0.0 % \%\}"}
EndConfig { canon-bjc-6100-stp }

