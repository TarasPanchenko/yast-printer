Config { lexmark-z11 }
	Param {
		-sDEVICE=z11
		-sBPP=1
		-sCOLOR=mono
	}
	UseOption { cycling }			Text { Cycled options (1) }
	UseOption { sub-cyc }			Text { Syb-cycled options (2) }
	UseOption { gs-cdj500-all-parameters } Text { Option that will be pasted (3) }
	UseOption { gs-pjxl300-all-parameters } Text { UseOption in PasteOption (4) }
EndConfig { lexmark-z11 }

Config { lexmark-z11-x }
    Param { -sDEVICE=stp }
