Printer { canon-bjc-6100 }
	UseConfig { canon-bjc-6100-mono } 	Text { Monochromatic printing (1) } Queue { mono }
	UseConfig { canon-bjc-6100-color } 	Text { Color printing (2) } Queue { color }
	UseConfig { canon-bjc-6100-photo } 	Text { Photo printing (3) } Queue { high }
EndPrinter { canon-bjc-6100 }

Printer { canon-bjc-6200 }
	UseConfig { canon-bjc-6200-mono } Text { Monochromatic printing (1) } Queue { mono }
	UseConfig { canon-bjc-6200-color } Text { Using PasteConfig (2) } Queue { color }
	UseConfig { canon-bjc-6300-color } Text { Well this is only a test (3) } Queue { photo }
EndPrinter { canon-bjc-6200 }

Printer { lexmark-z50 }
	Comment { <P>Unsupported GDI printer</P> }
EndPrinter { lexmark-z50 }

Printer { lexmark-z11 }
	UseConfig { lexmark-z11 }   Text { Monochromatic (1) }
	UseConfig { lexmark-z11-x } Text { Monogroomatic (2) } Queue { }
EndPrinter { lexmark-z11 }
