## YCP module configure.in.in

## Initialize
@YAST2-INIT-COMMON@
@YAST2-INIT-YCP@
@YAST2-INIT-PROGRAM@

# need cups/ipp.h -> cups, cups-devel
yast_found_ipp=no
AC_CHECK_HEADER(cups/ipp.h,[yast_found_ipp=yes])
if [[ "$yast_found_ipp" = "no" ]]; then
 AC_MSG_ERROR(Header file cups/ipp.h missing. Please install the package cups-devel.)
fi

# check for doxygen
AC_PATH_PROG(DOXYGEN, doxygen, false)
if test "$DOXYGEN" = "false"; then
    AC_MSG_ERROR([doxygen is not installed])
fi

## some common checks
@YAST2-CHECKS-COMMON@
@YAST2-CHECKS-YCP@
@YAST2-CHECKS-PROGRAM@

AM_PROG_LEX
AC_CHECK_PROGS(YACC, 'bison -y -d -v' byacc, false)

AX_CHECK_DOCBOOK
## and generate the output...
@YAST2-OUTPUT@
