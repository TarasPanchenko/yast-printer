{
    string filename = (string)SCR::Read (.file.open, ["tests/Postscript.ppd.gz", "."]);
    map groupsmap = (map)SCR::Read (.file.option_groups, filename);
    y2error ("groupsmap: %1", groupsmap);
    list groups = groupsmap["data"]:[];
    foreach (any g, groups, ``{
	y2error ("Group %1: %2", g, SCR::Read (.file.options, [filename, g]));
    });
    y2error ("All groups: %1", SCR::Read (.file.options, [filename, ""]));
}
