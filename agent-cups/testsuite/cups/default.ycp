{
    list pr = [];
    list cl = [];

    SCR::Write (.cups.printers, []);
    SCR::Write (.cups.classes, []);

    pr = [
	$[
	    "Name":"lp0",
	    "Uri":"parallel:/dev/lp0",
	    "ppd":"/usr/share/cups/model/epson9.ppd",
	    "options" : $[],
	    ],
	$[
	    "Name":"lp1",
	    "Uri":"parallel:/dev/lp0",
	    "ppd":"/usr/share/cups/model/epson9.ppd",
	    "options" : $[],
	    ],
	$[
	    "Name":"lp2",
	    "Uri":"parallel:/dev/lp0",
	    "ppd":"/usr/share/cups/model/epson9.ppd",
	    "options" : $[],
	    ],
	];
    SCR::Write (.cups.printers, pr);
    SCR::Write (.cups.default_dest, "lp0");
    y2error ("%1", SCR::Read (.cups.default_dest));
    SCR::Write (.cups.default_dest, "lp1");
    y2error ("%1", SCR::Read (.cups.default_dest));
    SCR::Write (.cups.default_dest, "lp2");
    y2error ("%1", SCR::Read (.cups.default_dest));
    SCR::Write (.cups.default_dest, "lp3");
    y2error ("%1", SCR::Read (.cups.default_dest));
}
