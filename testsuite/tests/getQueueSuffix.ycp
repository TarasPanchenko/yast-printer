{
    import "Pkg";

    include "testsuite.ycp";
    import "Printer";

    list queues = [
	$[ "name" : "lp", ],
	$[ "name" : "other", ],
	];

    Printer::printcap = [
	$[  "name" : "color",	],
	$[  "name" : "high",	],
    ];

    DUMP (sformat ("-->%1<--", Printer::getQueueSuffix (queues)));

    Printer::printcap = [
	$[  "name" : "lp",	],
	$[  "name" : "color",	],
	$[  "name" : "high",	],
    ];

    DUMP (Printer::getQueueSuffix (queues));

    Printer::printcap = [
	$[  "name" : "lp1",	],
	$[  "name" : "color3",	],
	$[  "name" : "high2",	],
    ];

    DUMP (Printer::getQueueSuffix (queues));

    Printer::printcap = [
	$[  "name" : "lp2",	],
	$[  "name" : "other1",	],
	$[  "name" : "high5",	],
    ];

    DUMP (Printer::getQueueSuffix (queues));
}
