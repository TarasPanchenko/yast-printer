{
    import "Pkg";

    import "Printerlib";
    include "testsuite.ycp";
    map EXEC = $[ "target": $["bash_output":$["stdout":"root\n", "stderr":"", "exit" : 0]]];
    TEST (``(Printerlib::amIRoot ()), [$[], $[], EXEC], 0);
    EXEC = $[ "target": $["bash_output":$["stdout":"nobody\n", "stderr":"", "exit" : 0]]];
    TEST (``(Printerlib::amIRoot ()), [$[], $[], EXEC], 0);
}
