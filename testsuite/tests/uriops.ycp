/**
 * File:
 *  uriops.ycp
 *
 * Module:
 *  Printer configurator
 *
 * Summary:
 *  Tests for basic translation functions.
 *
 * Authors:
 *  Petr Blahos <blahos@suse.cz>, 2001
 *
 * $Id$
 *
 * Test case for operations with printer uri.
 */

{
    include "testsuite.ycp";
    import "Printerlib";

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriType (nil));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriType (""));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriType ("parallel:/dev/null"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriType ("samba://user:pass@x.y.z/printer"));

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriDevice (nil));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriDevice (""));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriDevice ("parallel:/dev/null"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriDevice ("samba://user:pass@x.y.z/printer"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriDevice ("serial://dev/ttyS0?baudrate=9600"));

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriRemoteQueue ("lpd://gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriRemoteQueue ("ipp://cups.suse.de/printer/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriRemoteQueue ("samba://user:pass@gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriRemoteQueue ("novell://user@gorgon/lpz"));

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriBaudrate ("serial://dev/ttyS0?baudrate=9600"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriBaudrate ("serial://dev/ttyS0?baudrate=96000"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriBaudrate ("serial://dev/ttyS0"));

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("lpd://gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("ipp://cups.suse.de/printer/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("samba://user:pass@gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("novell://user@gorgon/lpz"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("socket://hp4100dn.suse.de/"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("socket://hp4100dn.suse.de"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriHost ("socket://hp4100dn.suse.de:9100"));

    y2debug ("aNY_OutPuT=%1", Printerlib::getUriUsernamePass ("samba://user:pass@gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriUsernamePass ("samba://user@gorgon/lp"));
    y2debug ("aNY_OutPuT=%1", Printerlib::getUriUsernamePass ("samba://gorgon/lp"));

    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0", "baudrate"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=15", "baudrate"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=15+bits=7", "baudrate"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?bits=7+baudrate=15", "baudrate"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=15", "baudrate"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=15+add=a a a+bits=15", "add"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=15+add=a a a+bits=15", "bits"));

    y2error ("aNY_OutPuT=%1", Printerlib::getUriPort ("socket://hp4100dn.suse.de:9100"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriPort ("ipp://zealot.suse.cz:666/printer/1"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriPort ("ipp://zealot.suse.cz/printer/1"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriPort ("socket://hp4100dn.suse.de"));

    y2error ("aNY_OutPuT=%1", Printerlib::getUriDevice ("serial:/dev/ttyS0?baudrate=9600+ty=ixon -imaxbel -ixany -ixoff"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriBaudrate ("serial:/dev/ttyS0?baudrate=9600+ty=ixon -imaxbel -ixany -ixoff"));
    y2error ("aNY_OutPuT=%1", Printerlib::getUriValue ("serial:/dev/ttyS0?baudrate=9600+ty=ixon -imaxbel -ixany -ixoff", "ty"));

}
