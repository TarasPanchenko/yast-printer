{
    import "Pkg";

    include "printer/testpage.ycp";
    include "testsuite.ycp";

    map READ = $[ "target" : $[ "tmpdir" : "/tmp/dir",
                                "stat" : $[] ] ];

    TEST (``(preparePsTestPage ("lp", "/tmp/file", false)), [READ, $[], $[]], 0);
    TEST (``(preparePsTestPage ("xxx", "/tmp/file", true)), [READ, $[], $[]], 0);
}
