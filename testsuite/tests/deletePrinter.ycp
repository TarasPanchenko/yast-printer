    DUMP (Printer::DeletedNames ());

    Printer::printcap = [
	$[ "name" : "1", "saved" : $[ "printcap_part" : "yes"]],
	$[ "name" : "2", ],
	$[ "name" : "3", ],
	$[ "name" : "4", ],
	];
    Printer::index = 0;
    Printer::deletePrinter ();
    DUMP (Printer::printcap);
    DUMP (Printer::DeletedNames ());
}
