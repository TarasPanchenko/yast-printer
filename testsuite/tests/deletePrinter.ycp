{
    import "Pkg";

    include "testsuite.ycp";
    import "Printer";

    Printer::printcap = [
	$[ "name" : "1", ],
	$[ "name" : "2", ],
	$[ "name" : "3", ],
	$[ "name" : "4", ],
	];
